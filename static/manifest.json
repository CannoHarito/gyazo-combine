{
  "name": "Gyazo Combine",
  "short_name": "GyazoCombine",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#efedea",
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "any",
      "type": "image/svg+xml"
    }
  ],
  "scope": "/",
  "share_target": {
    "action": "/upload",
